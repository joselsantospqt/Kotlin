<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.directory.DirectoryFragment">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="411dp"
        android:layout_height="277dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/btnArquivo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver Arquivos Salvos"
        app:layout_constraintBottom_toTopOf="@+id/btnDescripto"
        app:layout_constraintEnd_toEndOf="@+id/btnDescripto"
        app:layout_constraintStart_toStartOf="@+id/btnDescripto"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnDescripto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descriptografar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnArquivo" />
</androidx.constraintlayout.widget.ConstraintLayout>